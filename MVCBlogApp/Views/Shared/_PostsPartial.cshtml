@model IEnumerable<MVCBlogApp.Models.Post>

<div class="col-md-4">
    <div class="content-section">
        <h3>Latest Posts</h3>
        <p class="text-muted">These are the latest posts</p>
            @if (Model.Count() == 0)
            {
                <p>There are no posts</p>
            }
            else
            {
                <ul>
                    @foreach (var item in Model)
                    {
                        <li>
                            @Html.DisplayFor(modelItem => item.Title)
                        </li>
                    }
                </ul>
            }           
    </div>
</div>
